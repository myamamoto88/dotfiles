[user]
	name = myamamoto88
	email = myamamoto@aiming-inc.com

[ghq]
	root = /Users/myamamoto/src
	root = ~/.go/src/

[credential]
	helper = osxkeychain

[core]
	excludesfile = /Users/myamamoto/.gitignore

[color]
  diff        = auto
  grep        = auto
  branch      = auto
  status      = auto
  interactive = auto

[pull]
  rebase = true

[alias]
  pull = pull --ff-only
  branches = branch -a
  tags = tag
  stashes = stash list
  remotes = remote -v

  unstage = reset -q HEAD --
  discard = checkout --
  uncommit = reset --mixed HEAD~
  amend = commit --amend

  graph = log --graph -10 --branches --remotes --tags  --format=format:'%Cgreen%h %Cresetâ€¢ %<(75,trunc)%s (%cN, %cr) %Cred%d' --date-order
  precommit = diff --cached --diff-algorithm=minimal -w
